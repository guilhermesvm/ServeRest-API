{
    "request":{
       "method":"POST",
       "url":"/usuarios"
    },
    "response": {
        "status": 201,
        "headers": { "Content-Type": "application/json"},
        "jsonBody": {
            "id": 99,
            "nome": "{{{jsonPath request.body '$.name'}}}",
            "email": "{{{jsonPath request.body '$.email'}}}",
            "password": "{{{jsonPath request.body '$.password'}}}",
            "administrador": "{{{jsonPath request.body '$.administrador'}}}"
        }
    }
}